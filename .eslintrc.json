{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": ["off"]
  },
  "ignorePatterns": ["out/**/*.js"]
}
